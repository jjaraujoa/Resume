{UTF-8}
Población  = A FUNCTION OF( crecimiento poblacional,-Muertes,-Nuevos consumidores,-Nuevos criminales\
		,Rehabilitacion con,Rehabilitacion criminales,Total) ~~|
Población= INTEG (
	crecimiento poblacional+Rehabilitacion con+Rehabilitacion criminales-Muertes-Nuevos consumidores\
		-Nuevos criminales,
		30000)
	~	personas
	~		|

Total=
	Consumidores+Criminales+Población
	~	personas
	~		|

crecimiento poblacional=
	tasa de crecimiento poblacional*(Población+Criminales+Consumidores)
	~	personas/year
	~		|

inversion en bienestar=
	0.25*Presupuesto
	~	PeriCoin
	~		|

Presupuesto=
	Población*0.005
	~	PeriCoin
	~		|

Inversión real contra oferta=
	Inversión contra oferta*(1-(Corrupción*Cultura de la ilegalidad))
	~	PeriCoin
	~		|

Tasa de reclutamiento=
	0.05
	~	1/year
	~		|

Inversión contra oferta=
	0.75*Presupuesto
	~	PeriCoin
	~		|

Aux 3=
	1
	~	personas/(year*PeriCoin)
	~		|

Capturas=
	Aux 3*Inversión real contra oferta/10
	~	personas/year
	~		|

Rehabilitacion criminales=
	IF THEN ELSE(Criminales<=0, 0, Criminales*Tasa de rehabilitacion cri+Capturas )
	~	personas/year
	~		|

Salida cri=
	IF THEN ELSE(Criminales<=0, 0, tasa de salida*Criminales )
	~	personas/year
	~		|

Efecto Inversion Exp=
	EXP(-((Aux*inversion en bienestar*(1-Corrupción)-145))/800)-0.2
	~	1
	~		|

Aux=
	1
	~	1/PeriCoin
	~		|

Aux 2=
	1
	~	personas/Crimenes
	~		|

Cultura de la ilegalidad=
	Efecto crimenes*Efecto Inversion Exp
	~	1
	~		|

Efecto crimenes=
	IF THEN ELSE( "Waluigi (Crimenes relativos)" < 0.44928, POWER( ("Waluigi (Crimenes relativos)"\
		*Aux 2 + 0.1) , 5)*20 + 1 , IF THEN ELSE( "Waluigi (Crimenes relativos)"< 0.99845, \
		POWER( ("Waluigi (Crimenes relativos)"*Aux 2 - 1.09856+0.1) , 5)*20 + 3  , 3))
	~	1
	~		|

Crimenes relacionados con drogas=
	(tasa de delincuencia consumidores*Consumidores)+(tasa de delincuencia criminales*Criminales\
		)
	~	Crimenes
	~		|

tasa de captacion por criminales=
	0.1
	~	1/year
	~		|

"Waluigi (Crimenes relativos)"=
	Crimenes relacionados con drogas/(Población)
	~	Crimenes/personas
	~		|

Nuevos consumidores=
	(Captación espontanea+Captación de consumidores)*Cultura de la ilegalidad
	~	personas/year
	~		|

Nuevos criminales=
	(Tasa de reclutamiento*fraccion de criminales*Población*Cultura de la ilegalidad)
	~	personas/year
	~		|

Captación de consumidores=
	(Población*Tasa de captación de consumidores*fraccion de Consumidores)+(fraccion de criminales\
		*tasa de captacion por criminales
*Población)
	~	personas/year
	~		|

Consumidores= INTEG (
	Nuevos consumidores-Rehabilitacion con-salida Con,
		3000)
	~	personas
	~		|

Criminales= INTEG (
	Nuevos criminales-Rehabilitacion criminales-Salida cri,
		500)
	~	personas
	~		|

Tasa de rehabilitacion cri=
	0.0009
	~	1/year
	~		|

fraccion de Consumidores=
	Consumidores/(Población+Consumidores+0.001)
	~	1
	~		|

Rehabilitacion con=
	IF THEN ELSE(Consumidores<=0, 0, Consumidores*Tasa de rehabilitacion con)
	~	personas/year
	~		|

salida Con=
	IF THEN ELSE(Consumidores<=0, 0, Consumidores*tasa de salida consumidores)
	~	
	~		|

Tasa de rehabilitacion con=
	0.001
	~	1/year
	~		|

Corrupción=
	0.5
	~	1
	~		|

tasa de crecimiento poblacional=
	0.02
	~	
	~		|

tasa de muertes=
	0.006
	~	
	~		|

Muertes=
	tasa de muertes*Población
	~	
	~		|

tasa de delincuencia consumidores=
	0.001
	~	Crimenes/personas
	~		|

tasa de delincuencia criminales=
	5
	~	Crimenes/personas
	~		|

tasa de salida=
	0.016
	~	1/year
	~		|

fraccion de criminales=
	Criminales/(Población+Criminales+0.001)
	~	1
	~		|

tasa de salida consumidores=
	0.01
	~	
	~		|

Captación espontanea=
	Población*tasa de captación expontanea
	~	personas/year
	~		|

Tasa de captación de consumidores=
	0.01
	~	1/year
	~		|

tasa de captación expontanea=
	1e-05
	~	1/year
	~		|

